<?xml version="1.0"?>

<launch>
  <param name="use_sim_time" value="true"/>
  <node name="bag_play" pkg="rosbag" type="play" args="--clock -r 0.05 /home/aisl/research/research_storage/2017-12-15/test_data_sim/test_data_sim.bag" required="true"/>
    
  <node name="elas" pkg="elas_ros" type="elas_ros">
    <remap from="stereo" to="mobile_base/camera"/>
    <remap from="image" to="image_raw"/>
    <param name="image_transport" value="compressed"/>
  </node>
  
  <node name="bag_record" pkg="rosbag" type="record" args="/elas/depth /elas/disparity"/>
</launch>
